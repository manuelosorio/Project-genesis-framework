doctype html
html(lang='en')
  head
    meta(charset='utf-8')
    meta(http-equiv='x-ua-compatible', content='ie=edge')
    meta(name='viewport', content='width=device-width, initial-scale=1.0')
    meta(name='author', content='Manuel Osorio')
    meta(name='description', content='Project Genesis is a css framework meant to be the the starting point for your projects.')
    meta(name='keywords', content='Manuel Osorio, Project Genesis, Css Framework, Genesis, Project, Web Framework ')
    meta(name='robots', content='index, follow')
    link(rel='stylesheet', href='assets/css/project-genesis.min.css')
    link(rel='stylesheet', href='assets/css/app.min.css')


    // Start Social Media Meta Tags
    meta(property='og:title', content='Project Genesis | Css Framework')
    meta(property='og:description', content='Project Genesis is a css framework meant to be the the starting points for your projects.')
    meta(property='og:image', content='https://manuelosorio.github.io/osorio_m_final-project/assets/img/projects/project-genesis/thumb.png')
    meta(property='og:url', content='')
    meta(name='twitter:title', content='Project Genesis | Css Framework')
    meta(name='twitter:description', content='Project Genesis is a css framework meant to be the the starting points for your projects.')
    meta(name='twitter:image', content='https://manuelosorio.github.io/osorio_m_final-project/assets/img/projects/project-genesis/thumb.png')
    meta(name='twitter:card', content='summary_large_image')
    // End Social Media Meta Tags
    
  body
    .nav.default
      .left
        .logo Project Genesis
      .right
        .container
          - list = [{ link: 'index.html', i: 'Home' }, { link: 'colors.html', i: 'Colors' }, { link: 'video.html', i: 'html5 Video' }, { link: '#4', i: 'Item 4' }]
          each item in list
            a(href=item.link class=item.class) #{item.i}
            |
            |
        .menu.default#menu
          .menu-bar
          .menu-bar
          .menu-bar
    .page-content
      .row
        .col.col-10
          .container
            #video_player
              video#video()
                source(src='https://media.w3.org/2010/05/sintel/trailer.mp4', type='video/mp4' )
                // Safari / iOS, IE9
                source(src='https://media.w3.org/2010/05/sintel/trailer.webm', type='video/webm')
                // Chrome10+, Ffx4+, Opera10.6+
                source(src='https://media.w3.org/2010/05/sintel/trailer.ogy', type='video/ogg')
                // Firefox3.6+ / Opera 10.5+
                .loading
                  | I have a bad feeling about this... 
                  br
                  | ... your browser doesn't support the video format!
              #video_controls_bar
                button#playpausebtn
                  i.pg-play
                input#seekslider(type='range', min='0', max='100', value='0', step='1')
                span#time
                  span#cTimeText 00:00
                  | / 
                  span#dTimeText 00:00
                button#mutebtn
                  i.pg-max-vol
                input#volumeSlider(type='range', min='0', max='100', value='100', step='.05')
                button#fullscreenbtn
                  i.pg-enter-fullscrn


      .row
        .col.col-10
          include ../_docs/video.html


      .footer
        .row
          .col.col-12.col-md-12
            p Copyright 2016-2018 Manuel Osorio
        .row
          .col.col-4.col-md-4.col-sm-4
            .logo Project Geneis
          .col.col-4.col-md-4.col-sm-4
          .col.col-4.col-md-4.col-sm-4
            .links
              - list = [{ link: 'index.html', i: 'Home' }, { link: 'colors.html', i: 'Colors' }, { link: 'video.html', i: 'html5 Video' }, { link: '#4', i: 'Item 4' }]
              each item in list
                a(href=item.link class=item.class) #{item.i}
                |
                |
    script(src='https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js')
    script(src="assets/js/highlight.pack.js")
    script hljs.initHighlightingOnLoad();
    script(src='assets/js/project-genesis.js')
